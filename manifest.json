 {
  "name": "jQuery DOM",
  "version": "1",
  "manifest_version":2,
  "description": "Manipulate the DOM when the page is done loading",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
  	"css": ["bootstrap.min.css"],
    "js": [ "jquery.min.js", "bootstrap.min.js" ,"popup.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}